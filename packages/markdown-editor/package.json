{
  "name": "@ficusjs/cms-markdown-editor",
  "version": "1.0.0",
  "description": "Markdown editor for FicusJS CMS",
  "main": "index.js",
  "scripts": {
    "build": "rollup -i src/main.mjs -o dist/editor.mjs -f es -p \"node-resolve={}\" -p \"terser={mangle: { keep_fnames: true, toplevel: true, module: true }}\" -p \"filesize={}\""
  },
  "author": "Matt Levy",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.0.6",
    "escape-string-regexp": "5.0.0",
    "marked": "4.0.6",
    "rollup": "2.60.1",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-terser": "7.0.2",
    "standard": "16.0.4"
  }
}
